{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {},
  "rules": {
    "typedef": {
      "options": [
        "call-signature",
        "arrow-call-signature"//,
        // "array-destructuring",
        // "arrow-parameter",
        // "member-variable-declaration",
        // "object-destructuring",
        // "parameter",
        // "property-declaration",
        // "variable-declaration"
      ],
      "severity": "error"
    },
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}
